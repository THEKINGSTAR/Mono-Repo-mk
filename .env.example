# CambioML Computer Use Agent Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Required - Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
DATABASE_URL=postgresql://cambioml:password@localhost:5432/cambioml
POSTGRES_DB=cambioml
POSTGRES_USER=cambioml
POSTGRES_PASSWORD=password

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=cambioml123

# Application Configuration
DEBUG=true
LOG_LEVEL=info

# VNC Configuration
VNC_PASSWORD=cambioml123

# Security (generate strong passwords for production)
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here

# Optional: External Services
# SENTRY_DSN=your-sentry-dsn-here
PROMETHEUS_ENABLED=false

# Production Settings (set to false for development)
PRODUCTION=false
